{
	"geode": "5.0.0-beta.1",
	"gd": {
		"win": "2.2081",
		"android": "2.2081",
		"mac": "2.2081",
		"ios": "2.2081"
	},
	"version": "v1.2.4",
	"id": "alphalaneous.editorsounds",
	"name": "Editor Sounds",
	"developer": "Alphalaneous",
	"description": "Adds sounds to the editor!",
	"resources": {
		"files": [
			"audio/*.ogg"
		]
	},
	"links": {
        "homepage": "https://linktr.ee/Alphalaneous",
        "source": "https://github.com/Alphalaneous/Editor-Sounds",
        "community": "https://discord.gg/Txn2CA5dHF"
    },
	"tags": [
		"editor", "enhancement"
	],
	"dependencies": {
		"geode.node-ids": ">=v1.20.0"
	},
	"settings": {
		"enable-custom-sounds": {
			"type": "bool",
			"name": "Enable Custom Sounds",
			"description": "Allows you to point to a different folder containing the sounds you want.",
			"default": false
		},
		"custom-sound-path": {
			"type": "folder",
			"name": "Custom Sound Path",
			"description": "The path containing all your sounds",
			"enable-if": "enable-custom-sounds",
			"default": ""
		}
	}
}
